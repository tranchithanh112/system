						<PulseChannelClickAction>
							<Bindings>
								<FrameBinding TargetPropertyName="ChannelNumber" ExportedPropertyName="Mimic_Reset"/>
							</Bindings>
						</PulseChannelClickAction>
						<CloseFrameUserAction/>
