<?xml version="1.0" encoding="utf-8"?>
<RootElement Width="135" Height="45" PraxGraphVersion="1.0" ColorTableFile="..\..\Standard.colortable">
	<Children>
		<Rectangle Width="135" Height="45" FillColor="Transparent" CornerRoundRadius="5">
			<Bindings>
				<EASButtonStatusBinding TargetPropertyName="FillColor" ButtonType="CallAll">
					<ConvertParams>
						<BoolToColorConvertParams ColorWhenFalse="Transparent" ColorWhenTrue="IndicationLine"/>
					</ConvertParams>
				</EASButtonStatusBinding>
			</Bindings>
		</Rectangle>
		<Button Width="135" Height="45" Text="Call All" BorderColor="Transparent" FillColor="Transparent" TextColor="TextActive" Font="Praxis;22.0;N;N;N">
			<Bindings>
				<FrameBinding TargetPropertyName="TextColor" ExportedPropertyName="TextColor"/>
			</Bindings>
			<ClickActions>
				<EASCommandClickAction EASCommands="Call All"/>
			</ClickActions>
		</Button>
	</Children>
</RootElement>
